
<%= form_for @voice_actor do |f| %>
<%= f.label :name %>
<%= f.text_field :name, :autofocus => true, :required => true %>

<%= f.label :nationality %>
<%= f.text_field :nationality %>

<%= f.label :character_ids %>
<%= f.select :character_ids, Character.pluck(:name, :id), :include_blank => true %>

<%= f.label :image %>
<%= f.url_field :image %>

<%= f.submit %>


<% end %>
